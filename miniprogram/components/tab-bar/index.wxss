.custom-tabbar {
  bottom: 0;
  display: flex;
  width: 100%;
  position: relative;
  box-sizing: border-box;
  transition: all .25s ease-in-out;
}

.custom-tabbar .item{
  z-index: 999;
  width: 50%;
  height: 54rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 20rpx;
  color: #b1b1b1;
}

